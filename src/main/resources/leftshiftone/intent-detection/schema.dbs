namespace incoming {
    class Request {
        identityIds: [string]
        text: string
        language: string
        minScore: float
        noOfMatches: int = 1
    }
}
namespace outgoing {
    class Response {
        matches: [Match]
    }
    class Match {
        identityId: string
        intentId: string
        score: float
        language: string
        utterance: string
    }
}
